import 'package:flutter/material.dart';

void main() {
  runApp(const MyApp());
}

class MyApp extends StatelessWidget {
  const MyApp({ Key? key }) : super(key: key);
  @override
  Widget build(BuildContext context) {
    return MaterialApp(
      title: "FlutterLabkom",
      debugShowCheckedModeBanner: false, //menghapus tanda debug diujung hp
      home: Scaffold(appBar: (AppBar(title:const Text("FLUTTER LABKOM"),)
      ),
      body :Container(
        child: const Center(
          child: Text("Welcome to Stateless Widget"),
        ),
      )
      ),
      
    );
  }
}
